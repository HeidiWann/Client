                <Font name="System Bold" size="38.0" />
            </font>
        </Label>
        <Button fx:id="profileButton" layoutX="769.0" layoutY="17.0" mnemonicParsing="false" onAction="#showProfileWindow" prefHeight="45.0" prefWidth="48.0" style="-fx-background-radius: 50em;" textFill="WHITE">
